<Histogram_Page>:
    BoxLayout:
        orientation: 'vertical'
        spacing: 10
        padding: 10,40

        BoxLayout:
            size_hint: 1,.2
            orientation: 'vertical'

            Image:
                source: "images//logo.png"
                height: 300

        BoxLayout:
            orientation: 'vertical'
            id: loadingLayout

        Button:
            size_hint: .2,0.1
            pos_hint: { 'top' : .95, 'right': .60}
            text: "Back"
            on_press: root.get_back()

